{
  "targetDefaults": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "{projectRoot}/dist"
      ],
      "cache": true
    },
    "typecheck": {
      "dependsOn": [
        "^build",
        "^typecheck"
      ]
    },
    "lint": {
      "dependsOn": [
        "^build",
        "^lint"
      ]
    }
  },
  "defaultBase": "main"
}